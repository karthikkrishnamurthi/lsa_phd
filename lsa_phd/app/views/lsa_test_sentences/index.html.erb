<h1>Listing lsa_test_sentences</h1>

<table>
  <tr>
    <th>Lsa document</th>
    <th>Document</th>
    <th>Label</th>
    <th>Lsa label</th>
    <th>Precision</th>
    <th>Recall</th>
    <th>F measure</th>
    <th>Similarity</th>
    <th>Sentence</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @lsa_test_sentences.each do |lsa_test_sentence| %>
  <tr>
    <td><%= lsa_test_sentence.lsa_document_id %></td>
    <td><%= lsa_test_sentence.document_id %></td>
    <td><%= lsa_test_sentence.label %></td>
    <td><%= lsa_test_sentence.lsa_label %></td>
    <td><%= lsa_test_sentence.precision %></td>
    <td><%= lsa_test_sentence.recall %></td>
    <td><%= lsa_test_sentence.f_measure %></td>
    <td><%= lsa_test_sentence.similarity %></td>
    <td><%= lsa_test_sentence.sentence %></td>
    <td><%= link_to 'Show', lsa_test_sentence %></td>
    <td><%= link_to 'Edit', edit_lsa_test_sentence_path(lsa_test_sentence) %></td>
    <td><%= link_to 'Destroy', lsa_test_sentence, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

